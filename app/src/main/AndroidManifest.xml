<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.activitytracker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Noaction">
        <activity android:name=".Reminder.Remindermain"></activity>
        <activity
            android:name=".Dashboard.add_notes"
            android:label="@string/title_activity_add_notes"
            android:theme="@style/Theme.Noaction.NoActionBar" />
        <activity
            android:name=".Dashboard.Note_details"
            android:label="@string/title_activity_note_details"
            android:theme="@style/Theme.Noaction.NoActionBar" />
        <activity android:name=".VerifyEmail" />
        <activity android:name=".Dashboard.DashboardMainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>